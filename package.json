{
  "name": "wisconsin-zoo",
  "version": "0.0.1",
  "dependencies": {
    "haw": "~0.5.0",
    "t7e": "~0.3.1",
    "spine": "1.0.7",
    "zooniverse": "~0.7.1",
    "zootorial": "~0.2.0",
    "publisssh": "~0.2.5"
  }, 
  "scripts": {
    "start": "haw serve --port 6001",
